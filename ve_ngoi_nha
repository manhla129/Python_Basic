import turtle
tl =turtle.Turtle()
tl.shape('turtle')
turtle.bgcolor('cyan')

tl.penup()
tl.goto(-300,-300)
tl.pendown()

tl.left(90)
tl.fillcolor('blue')
tl.begin_fill()
tl.forward(200)
tl.right(90)
tl.forward(150)
tl.right(90)
tl.forward(200)
tl.end_fill()

tl.right(90)
tl.forward(40)
tl.right(90)
tl.fillcolor('green')
tl.begin_fill()
tl.forward(120)
tl.left(90)
tl.forward(70)
tl.left(90)
tl.forward(120)
tl.end_fill()
